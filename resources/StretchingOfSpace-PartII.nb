Cell[CellGroupData[{

Cell["The Interpretation of the Riemann Sum", "Subsection",
 CellChangeTimes->{{3.884363843113154*^9, 3.884363851533698*^9}, 
   3.884364360162833*^9, {3.8843650264479513`*^9, 3.884365031083378*^9}, {
   3.8843655422517767`*^9, 3.8843655511185913`*^9}, {3.884365620152521*^9, 
   3.884365626283936*^9}, {3.884370251858327*^9, 3.884370267101452*^9}, {
   3.8843704350511703`*^9, 3.884370442510037*^9}, {3.884695541217863*^9, 
   3.884695541419017*^9}, {3.884697016099038*^9, 3.884697019238081*^9}, {
   3.884697689560907*^9, 
   3.884697695107274*^9}},ExpressionUUID->"d75e4436-ad5c-4ba3-a091-\
9c43eb22aafb"],

Cell[TextData[{
 "The sum\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox[
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"k", "=", "0"}], 
      RowBox[{"n", "-", "1"}]],
     FontSize->18], 
    RowBox[{
     StyleBox[
      RowBox[{"f", "(", 
       SubscriptBox["r", 
        RowBox[{"k", " "}]], ")"}],
      FontSize->16], 
     StyleBox["\[CapitalDelta]",
      FontSize->16], 
     StyleBox["\[VeryThinSpace]",
      FontSize->16], 
     StyleBox["r",
      FontSize->18]}]}], TraditionalForm]],ExpressionUUID->
  "035b7401-edae-4d6b-acba-3c61082ff8c9"],
 "\n\nwhere\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["r", "k"], "=", 
    RowBox[{
     SubscriptBox["r", "L"], "+", 
     RowBox[{
     "k", "\[VeryThinSpace]", "\[CapitalDelta]", "\[VeryThinSpace]", 
      "r"}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "390e2b16-ef07-4d54-b5c1-16fa526fb74f"],
 "\n\nand\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "r"}], "=", 
    FractionBox[
     RowBox[{
      SubscriptBox["r", "H"], "-", 
      SubscriptBox["r", "L"]}], "n"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "9390e56c-be13-4e01-8083-d551fdfb9fb3"],
 "\n\nhas the interpretation of being an area!\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox[
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"k", "=", "0"}], 
      RowBox[{"n", "-", "1"}]],
     FontSize->18], 
    RowBox[{
     StyleBox["width",
      FontSize->18], 
     StyleBox["\[CenterDot]",
      FontSize->18], 
     SubscriptBox[
      StyleBox["height",
       FontSize->18], "k"]}]}], TraditionalForm]],ExpressionUUID->
  "8772825a-99c9-4dcf-9817-3e7fe96e5617"],
 "\n\nwhere the width is ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalDelta]", "\[VeryThinSpace]", "r"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "937d9fd4-a3af-4666-b575-43d009420aef"],
 " and the ",
 Cell[BoxData[
  FormBox[
   RowBox[{"k", "\[VeryThinSpace]", "th"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "5db97709-a401-4301-9e39-0f78977712bd"],
 " height is ",
 Cell[BoxData[
  FormBox[Cell[TextData[Cell[BoxData[
    FormBox[
     RowBox[{"f", "(", 
      SubscriptBox["r", "k"], ")"}], TraditionalForm]],
    FormatType->TraditionalForm,ExpressionUUID->
    "d5a25792-7372-42f2-a8c7-ab0f41062e17"]],ExpressionUUID->
    "1c272d61-a1ac-415e-91df-e489658f7188"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "4211b3dd-af68-4745-b971-c7dd3c083e67"],
 ".\n\nYou can easily find many pictures on the internet illustrating this. \
Here is one:"
}], "Text",
